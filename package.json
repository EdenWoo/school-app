{
  "name": "enroller-app",
  "homepage": "https://EdenWoo.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src/*.(js|jsx)"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "antd": "^5.1.5",
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-modal": "^3.13.1",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.8",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.8.3",
    "stylelint": "^14.9.1",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "tailwind"
          ]
        }
      ],
      "no-invalid-position-at-import-rule": null
    }
  }
}
